<div class="col-sm-12">
  <div class="row">
    <div class="col">
      <h1>Articles</h1>
      <button class="btn btn-outline-primary scrape-btn">Get New Articles</button>
    </div>
  </div>
  <div class="row">
    <div class="col">
      {{#each data}}
        <div class="card">
          <div class="card-body">
            {{#each this}}
              {{#if title}}
                <div class="card-title">
                  <a href="{{link}}">{{title}}</a>
                </div>
              {{/if}}
              {{#if summary}}
                <div class="card-text">
                  <p>{{summary}}</p>
                </div>
              {{/if}}
            {{/each}}
          </div>
        </div>
        <br>
      {{/each}}
    </div>
  </div>
</div>

<script src="/js/index.js"></script>